package innerpackage;
class Outer2
{
	public void display()
	{
		class Inner
		{
			public void show()
			{
				System.out.println("Hello");
			}
		}
		Inner i=new Inner();
		i.show();
	}
}
public class Exampl3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      Outer2 o= new Outer2();
      o.display();
	}

}
